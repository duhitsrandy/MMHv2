{
  "name": "meetmehalfwaymobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "prestart": "bash -lc 'for p in 8081 8082 19000 19001; do lsof -ti tcp:$p | xargs kill -9 2>/dev/null || true; done; rm -rf $TMPDIR/metro-* node_modules/.cache/metro .expo 2>/dev/null || true'",
    "start": "expo start",
    "android": "expo start --android --port 8081",
    "preios": "bash -lc 'for p in 8081 8082 19000 19001; do lsof -ti tcp:$p | xargs kill -9 2>/dev/null || true; done; rm -rf $TMPDIR/metro-* node_modules/.cache/metro .expo 2>/dev/null || true'",
    "ios": "expo start --ios --port 8081 --clear",
    "ios:reset-sim": "bash -lc 'xcrun simctl shutdown all 2>/dev/null || true; xcrun simctl erase all 2>/dev/null || true; open -a Simulator'",
    "web": "expo start --web --port 8081",
    "dev": "npm run ios",
    "test": "jest --watchAll"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.14.15",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/native": "^7.1.6",
    "@stripe/stripe-react-native": "0.45.0",
    "@supabase/supabase-js": "^2.54.0",
    "expo": "~53.0.20",
    "expo-clipboard": "^7.1.5",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.7",
    "expo-location": "^18.1.6",
    "expo-router": "~5.1.4",
    "expo-secure-store": "^14.2.3",
    "expo-sharing": "^13.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "dotenv": "^17.2.1",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.9",
    "nativewind": "^4.1.23",
    "react-test-renderer": "19.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3"
  },
  "private": true
}
